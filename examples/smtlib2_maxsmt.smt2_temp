

( set-option :opt.maxsat_engine maxres )

( declare-fun x (  ) Real )
( declare-fun y (  ) Real )
( assert ( = x ( - y ) ) )

;declaration of additional variable for assert-soft

( declare-fun goal (  ) Real )
( assert-soft ( < x 0.0 ) :weight 1.0 :id goal )
( assert-soft ( < x y ) :weight 1.0 :id goal )
( assert-soft ( < y 0.0 ) :weight 1.0 :id goal )

( minimize goal :upper 2.0 )

( check-sat )
( get-objectives )

( exit )



